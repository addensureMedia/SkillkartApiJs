<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <script defer>
      const user = "<%= user.Name %>";
      const userrole = "<%= user.role %>";
      const useremail = "<%= user.Email %>";
      const user_id = "<%= user._id %>";
      const user_photo = "<%= user.photo %>";
    </script>

    <title>Skillkart | Dashboard</title>
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,400,0,0"
    />
    <link rel="stylesheet" href="../css/userprofile.css" />
    <script src="https://unpkg.com/validator@latest/validator.min.js"></script>

    <link rel="stylesheet" href="../css/celender.css" />
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="../script/celender.js" defer></script>
    <script src="../script/userprofile.js" defer></script>
  </head>
  <body>
    <div class="profileheader">
      <div class="profileheadertitle"></div>
      <div class="profiler">
        <a href="/"><div class="profileoption">Home</div></a>
        <a href="/"><div class="profileoption">Product</div></a>
        <div class="profileoption-btn" onclick="logout()">Logout</div>
      </div>
    </div>
    <div class="profilecontainer">
      <%- include('./Dashboardheader') -%>
      <div class="profilecontainerdashboard">
        <div class="profilecontaineractivity"></div>
        <div class="profilereviews">
          <div class="profilereviews-tile">Upcoming event</div>
          <div class="reviewscontainer">
            <div class="mentiesheader">
              <div class="mentiesheadercomponent">Mentor</div>
              <div class="mentiesheadercomponent">Date</div>
              <div class="mentiesheadercomponent">Product</div>
              <div class="mentiesheadercomponent">Room</div>
            </div>
            <div class="mentorscontaienr">
              <div class="menties-product-container">
                <div class="menties-product-mentor">
                  <img
                    src="https://cdn-icons-png.flaticon.com/512/1177/1177568.png"
                    alt=""
                    width="30px"
                  />
                  <div class="mentorname">Jagdeep singh</div>
                </div>
                <div class="menties-product-mentor">
                  <div class="mentorname">28 Aug 2022</div>
                </div>
                <div class="menties-product-mentor">
                  <div class="mentorname">B. Tech</div>
                </div>
                <div class="menties-product-mentor">
                  <div class="mentor-Room">Enter Room</div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="profilereviews">
          <div class="profilereviews-tile">Your purchase</div>
          <div class="reviewscontainer">
            <div class="mentiesheader">
              <div class="mentiesheadercomponent">Mentor</div>
              <div class="mentiesheadercomponent">Product</div>
              <div class="mentiesheadercomponent">Price</div>
              <div class="mentiesheadercomponent">Status</div>
            </div>
            <div class="mentorscontaienr">
              <div class="menties-product-container">
                <div class="menties-product-mentor">
                  <img
                    src="https://cdn-icons-png.flaticon.com/512/1177/1177568.png"
                    alt=""
                    width="30px"
                  />
                  <div class="mentorname">Jagdeep singh</div>
                </div>
                <div class="menties-product-mentor">
                  <div class="mentorname">B. Tech</div>
                </div>
                <div class="menties-product-mentor">
                  <div class="mentorname">7999</div>
                </div>
                <div class="menties-product-mentor">
                  <div class="mentor-status">Success</div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="sidebar"><%- include('celender') %></div>
    </div>
  </body>
</html>
